<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img"
    tools:context=".signupActivity">
    <TextView
        android:id="@+id/res_name_reg"
        style="@style/Widget.AppCompat.TextView"
        android:layout_width="289dp"
        android:layout_height="76dp"
        android:layout_marginTop="44dp"
        android:fontFamily="cursive"
        android:gravity="center"
        android:text="@string/res_name"
        android:textColor="#232323"
        android:textSize="70sp"
        android:textStyle="normal|bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/creation"
        android:layout_width="211dp"
        android:layout_height="61dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="create your account"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/res_name_reg" />

    <EditText
        android:id="@+id/first_name"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="First name"
        android:paddingStart="12dp"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:background="@drawable/editshape"
        android:textColorHint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/creation"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/last_name"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/editshape"
        android:ems="10"
        android:hint="Last name"
        android:inputType="textPersonName"
        android:paddingStart="12dp"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_name"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/email_reg"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/editshape"
        android:drawableStart="@drawable/ic_email"
        android:drawablePadding="5dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:paddingStart="12dp"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/last_name"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/password_reg"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/editshape"
        android:drawableStart="@drawable/ic_lock"
        android:drawablePadding="5dp"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:paddingStart="12dp"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_num"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/phone_num"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/editshape"
        android:drawableStart="@drawable/ic_phone"
        android:drawablePadding="5dp"
        android:ems="10"
        android:hint="Phone"
        android:inputType="phone"
        android:paddingStart="12dp"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_reg"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/confirm_password_reg"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/editshape"
        android:drawableStart="@drawable/ic_lock"
        android:drawablePadding="5dp"
        android:ems="10"
        android:hint="confirm password"
        android:inputType="textPassword"
        android:paddingStart="12dp"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password_reg"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/register"
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="@string/register_bt"
        app:backgroundTint="#E4FF8400"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/confirm_password_reg" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="226dp"
        android:layout_height="35dp"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:text="Already have an account ?"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register" />

    <TextView
        android:id="@+id/sign_in_reg"
        android:layout_width="107dp"
        android:layout_height="41dp"
        android:gravity="center"
        android:text="Sign in"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.0" />



</androidx.constraintlayout.widget.ConstraintLayout>